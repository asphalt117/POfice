@model IEnumerable<Domain.Entities.Contract>

@{
    ViewBag.Title = ViewBag.ContentTitle = "Договоры";
}
@if (Model.Count() > 0)
{
        <table class="table table-bordered table-hover mt-3">
            @foreach (var item in Model)
    {
        <tr>
            <td>
                Договор
                @Html.DisplayFor(modelItem => item.Num)
            </td>
        </tr>
}

        </table>
        }
        else
        {
        <h2>У вас нет действующих договоров</h2>}

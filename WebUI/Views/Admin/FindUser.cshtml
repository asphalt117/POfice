@model List<string>
@{
    ViewBag.Title = ViewBag.ContentTitle = "Поиск клиентов";
}
<h2>Код контрагента</h2>
<div>
    @using (Ajax.BeginForm("CustSearch", new { userid = Model.ElementAt(0), email = Model.ElementAt(1) }, new AjaxOptions { UpdateTargetId = "searchresults", Url = Url.Action("CustSearch", "Admin") }))
    {
        <input type="text" name="id" />
        <input type="submit" value="Поиск и привязка" />
    }
</div>
